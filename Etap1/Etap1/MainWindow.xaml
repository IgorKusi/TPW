<Window x:Class="Etap1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Etap1"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" ResizeMode="NoResize">
    <Grid>
        <Button Content="PLAY" Margin="685,8,15,0" VerticalAlignment="Top" Height="39" />
        <Button Content="RESET" Margin="685,62,15,0" VerticalAlignment="Top" Height="40" />
        <Slider Margin="604,300,-66,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="20" Minimum="1" Maximum="30" IsSnapToTickEnabled="True" AutoToolTipPlacement="BottomRight" AutoToolTipPrecision="0">
            <Slider.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Slider.RenderTransform>
        </Slider>
        <Rectangle Height="484" Margin="0,0,120,0" Stroke="Black" VerticalAlignment="Top" Uid="Map"/>
        <Button Content="FRAME" Margin="685,118,15,0" VerticalAlignment="Top" Height="40" />
    </Grid>
</Window>
