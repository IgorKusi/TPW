<Window x:Class="Etap1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Etap1"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" ResizeMode="NoResize">
    <Grid>
        <Button Content="PLAY" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top" Height="39" Width="100" />
        <Button Content="RESET" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top" Height="40" Width="100" />
        <Slider HorizontalAlignment="Left" Margin="-107,269,0,0" VerticalAlignment="Top" Width="336" RenderTransformOrigin="0.5,0.5" Height="20" Minimum="1" Maximum="30" IsSnapToTickEnabled="True" AutoToolTipPlacement="BottomRight" AutoToolTipPrecision="0">
            <Slider.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Slider.RenderTransform>
        </Slider>
        <Rectangle Height="484" Margin="120,0,0,0" Stroke="Black" VerticalAlignment="Top" Uid="Map"/>
    </Grid>
</Window>
